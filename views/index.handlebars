  <nav>
    <div class="nav-wrapper white">
        <a href="#" class="brand-logo center">Hang</a>
        <button id="logOut-btn" class="btn btn-primary yellow right">Log out</button>
        <li id="hello-there">Hello <span id="userName"></span></li>
        <ul id="nav-mobile" class="left hide-on-med-and-down"> </ul>

    </div>
  </nav>





     <div class="row">
    <div class="col s12 m6">
      <div class="card card-container white">
        <div class="card-content">
          <span class="card-title">Here are the hangs you've created:</span>
               <ul>
                {{#each hangs}}
                    <div>
                        <p><b>{{hangName}}</b></p>
                        <p>{{aboutHang}}</p>
                    </div>
                    <hr>
                {{/each}}
            </ul>
        </div>
      </div>
    </div>
  </div>



    <div class="wrap-container">
        <div class="col s12 m6 l6 xl6">
            <h3>Add a new hang:</h3>
            <form id="createHang">
                <div class="form-group">
                    <label for="hang-name">Hang Name:</label>
                    <input type="text" class="form-control" id="hang-name"  placeholder="New hang name">
                </div>
                <div class="form-group">
                    <label for="about-hang">What is this hang for?</label>
                    <textarea id="about-hang" class="form-control"  placeholder=""></textarea>
                </div>
                <button id="addHang" type="submit" class="btn btn-primary">Add</button>
            </form>
        </div>
    </div>

    </div>
</div>


<script type="text/javascript" src="/js/index.js" ></script> 



