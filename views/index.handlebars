        <div class="col-md-4 enter-hang">
            <h4>Add a new hang:</h4>
            <form id="createHang">
                <div class="form-group">
                    <label for="hang-name">Hang Name:</label>
                    <input type="text" class="form-control" id="hang-name"  placeholder="New hang name">
                </div>
                <div class="form-group">
                    <label for="about-hang">What is this hang for?</label>
                    <textarea id="about-hang" class="form-control"  placeholder=""></textarea>
                </div>
                   <div class="form-group">
                    <label for="event-recipient">Recipient email:</label>
                    <input type="text" class="form-control" id="event-recipient">
                </div>
                 <div class="form-group">
                    <label for="hang-time">Hang time:</label>
                    <input type="time" id="hang-time" class="form-control"  placeholder=""></textarea>
                </div>
                 <div class="form-group">
                    <label for="hang-date">Hang date:</label>
                    <input type="date" id="hang-date" class="form-control"  placeholder=""></textarea>
                </div>
                <button id="addHang" type="submit" class="btn btn-primary yellow darken-2">Add</button>
            </form>
        </div>


      <div class="wrap-container-calendar">
         <div class="col-md-4 calendar-form">
            <h4>Add a new event to your calendar:</h4>
            <form id="createEvent">
                <div class="form-group">
                    <label for="event-name">Event Name:</label>
                    <input type="text" class="form-control" id="event-name">
                </div>
             
                <div class="form-group">
                    <label for="event-date">Date:</label>
                    <input id="event-date" type="datetime" class="form-control"  placeholder="">
                </div>
                
                <div class="form-group">
                    <label for="start-time">Start time:</label>
                    <input id="start-time" type="text" class="timepicker"  placeholder=""></input>
                </div>
                <div class="form-group">
                    <label for="end-time">End time:</label>
                    <input id="end-time" type="datetime" class="timepicker"  placeholder=""></input>
                </div>
                <button id="addEvent" type="text" class="btn btn-primary timepicker">Add</button>

            </form>
    </div>

    <div id="caldiv">
                <div id="calBtn-div">
                    <button type="button" class="cal-btn btn btn-cstm" data-toggle="modal" data-target="#addEvent-modal">
                        New calendar item
                    </button>
                    <div class="cal-btn google-sync">
                        <button class="btn btn-cstm" id="authorize_button" style="display: none;">Sync with Google</button>
                        <button class="btn btn-cstm" id="signout_button" style="display: none;">Sign Out</button>
                        <pre id="content"></pre>
                    </div>
                </div>
                <div id="mycalendar">
                </div>
            </div>
        </div>


     <br>
<div class="card-hang">
  <div class="row valign-wrapper align-right">
    <div class="col s12 m6">
      <div class="card white">
        <div class="card-content black-text">
          <span class="card-title"><h4>Your upcoming hangs:</h4></span>
           <ul>
                {{#each hangs}}
                    <div>
                        <p><b>{{hangName}}</b></p>
                        <p>{{aboutHang}}</p>
                    </div>
                    <hr>
                {{/each}}
            </ul>
        </div>
      </div>
    </div>
  </div>
</div>

  <!-- Modal Structure -->

<div class="modal fade" id="pending-modal" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">You've been invited to these hangs:</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
            <p id="phang-name"></p>
            <p id="phang-about"></p>
            <p id="phang-date"></p>
            <p id="phang-time"></p>
            <button id="canDo">Can Do!</button>
      </div>
      
    </div>
  </div>
</div>




