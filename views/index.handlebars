        <div class="col-md-4 enter-hang">
            <h3>Add a new hang:</h3>
            <form id="createHang">
                <div class="form-group">
                    <label for="hang-name">Hang Name:</label>
                    <input type="text" class="form-control" id="hang-name"  placeholder="New hang name">
                </div>
                <div class="form-group">
                    <label for="about-hang">What is this hang for?</label>
                    <textarea id="about-hang" class="form-control"  placeholder=""></textarea>
                </div>
                   <div class="form-group">
                    <label for="event-recipient">Recipient email:</label>
                    <input type="text" class="form-control" id="event-recipient">
                </div>
                 <div class="form-group">
                    <label for="hang-time">Hang time:</label>
                    <input type="time" id="hang-time" class="form-control"  placeholder=""></textarea>
                </div>
                 <div class="form-group">
                    <label for="hang-date">Hang date:</label>
                    <input type="date" id="hang-date" class="form-control"  placeholder=""></textarea>
                </div>
                <button id="addHang" type="submit" class="btn btn-primary yellow darken-2">Add</button>

            </form>





        </div>
